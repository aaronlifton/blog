    "clsx": "^2.1.0",
    "cypress": "^13.6.4",
    "markdown-it-front-matter": "^0.2.3",
    "npm": "^10.4.0",
    "postcss-functions": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^7.1.0",
