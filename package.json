{
  "name": "fullstack-dev-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "create-db": "node --env-file=.env bin/create-db",
    "prepare": "husky",
    "format": "prettier --write .",
    "formatmd": "cbfmt --best-effort src/content/blog2/**.mdx && markdownlint-cli2 --fix \"src/content/blog2/**/*mdx\" \"#node_modules\"",
    "check": "prettier --check src && eslint . && astro check && tsc --noEmit",
    "test": "playwright test",
    "preview2": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "cypress": "npx cypress run"
  },
  "dependencies": {
