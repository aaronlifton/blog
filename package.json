    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/solid-js": "^4.0.1",
    "@astrojs/svelte": "^5.0.3",
    "@astrojs/tailwind": "^5.1.0",
    "@evilmartians/harmony": "^1.2.0",
    "@fontsource-variable/dm-sans": "^5.0.3",
    "@fontsource/comic-mono": "^5.0.7",
