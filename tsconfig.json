{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$util/*": ["src/util/*"],
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
      "$/*": ["src/*"]
    },
    "types": ["astro/client"],
    "jsx": "react-jsx"
  },
  "include": ["src/**/*", "plugins", "scripts", "tests"],
  "exclude": ["src/components/devicons", "src/tmpscripts", "src/tmp"]
}
