{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$/": ["./src/"],
      "$/*": ["./src/*"],
      "$i18n/*": ["./src/i18n/*"],
      "$utils/*": ["./src/utils/*"],
      "$assets/*": ["./src/assets/*"],
      "$components/*": ["./src/components/*"],
      "$layouts/*": ["./src/layouts/*"]
    },
    "types": ["astro/client"],
    "jsx": "react-jsx",
  },
  "include": ["src/**/*", "plugins", "scripts", "tests"],
  "exclude": ["src/components/devicons", "src/tmpscripts/**/*"]
}
