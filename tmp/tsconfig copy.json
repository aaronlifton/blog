{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*"],
  "compilerOptions": {
    "baseUrl": "/Users/aaron/Code/astro/fullstack-dev-blog",
    "strictNullChecks": true,
    "paths": {
      "$": [
        "./src"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "declarationMap": true
  }
}
