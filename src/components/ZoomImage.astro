---
import { ZoomIn } from "lucide-react";
import { default as InnerZoomImage } from "./ZoomImage.svelte";
interface Props {
  src: string;
  alt: string;
  caption: string;
}
const props: Props = Astro.props;
---

<InnerZoomImage {...props} src={props.src} client:load>
  <Fragment slot="icon"><ZoomIn /></Fragment>
</InnerZoomImage>
